{
	"name": "jquery-client",
	"version": "3.0.0",
	"description": "Minimal client-side user-agent parser.",
	"license": "MIT",
	"keywords": [
		"jquery-plugin"
	],
	"homepage": "https://gerrit.wikimedia.org/g/jquery-client",
	"files": [
		"src/",
		"AUTHORS.txt",
		"README.md",
		"LICENSE-MIT"
	],
	"scripts": {
		"authors": "git shortlog -se | sed 's/[[:space:]]*[0-9]*[[:space:]]//' && echo | grep -v jenkins > AUTHORS.txt",
		"doc": "jsdoc -c jsdoc.json",
		"lint": "eslint --cache --max-warnings 0",
		"test": "npm run lint . && karma start && npm run doc"
	},
	"devDependencies": {
		"eslint-config-wikimedia": "0.28.2",
		"jquery": "3.5.1",
		"jsdoc": "4.0.3",
		"jsdoc-class-hierarchy": "1.1.2",
		"jsdoc-wmf-theme": "1.0.1",
		"karma": "5.2.1",
		"karma-cli": "2.0.0",
		"karma-coverage": "2.0.3",
		"karma-firefox-launcher": "1.3.0",
		"karma-qunit": "4.1.1",
		"qunit": "2.11.1"
	}
}
