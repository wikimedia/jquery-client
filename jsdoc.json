{
	"opts": {
		"encoding": "utf8",
		"destination": "docs",
		"package": "package.json",
		"readme": "README.md",
		"recurse": true,
		"private": true,
		"template": "node_modules/jsdoc-wmf-theme",
		"class-hierarchy": {
			"showList": true
		}
	},
	"plugins": [
		"node_modules/jsdoc-wmf-theme/plugins/default"
	],
	"source": {
		"include": [ "src" ]
	},
	"templates": {
		"cleverLinks": true,
		"default": {
			"useLongnameInNav": true
		},
		"wmf": {
			"maintitle": "jQuery Client",
			"repository": "https://gerrit.wikimedia.org/g/jquery-client",
			"hideSections": [ "Events" ]
		}
	}
}
